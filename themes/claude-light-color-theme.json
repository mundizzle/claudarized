  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#73726D",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#7A937A"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": {
        "foreground": "#C08830"
      }
    },
    {
      "scope": ["constant.other", "constant.other.color"],
      "settings": {
        "foreground": "#CC7C5E"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#CC7C5E"
      }
    },
    {
      "scope": ["keyword.control", "keyword.operator"],
      "settings": {
        "foreground": "#CC7C5E"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#B38B2D"
      }
    },
    {
      "scope": ["entity.name.class", "entity.name.type", "support.class", "support.type"],
      "settings": {
        "foreground": "#4A9B92"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#CC7C5E"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#C08830"
      }
    },
    {
      "scope": ["variable", "variable.other", "variable.parameter"],
      "settings": {
        "foreground": "#9080B0"
      }
    },
    {
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#CC7C5E",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["support.constant", "support.variable"],
      "settings": {
        "foreground": "#4A9B92"
      }
    },
    {
      "scope": ["punctuation", "meta.brace"],
      "settings": {
        "foreground": "#73726D"
      }
    },
    {
      "scope": ["meta.function-call"],
      "settings": {
        "foreground": "#B38B2D"
      }
    },
    {
      "scope": ["entity.name.namespace", "entity.name.module"],
      "settings": {
        "foreground": "#4A9B92"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#CC7C5E",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#7A937A"
      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#73726D",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.list"],
      "settings": {
        "foreground": "#C08830"
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#4682D5"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#DC2626"
      }
    },
    {
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#C08830",
        "fontStyle": "strikethrough"
      }
    }
  ]
}
